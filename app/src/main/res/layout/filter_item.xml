<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tvItemToRv"
        android:layout_width="0dp"
        android:layout_height="@dimen/button_layout_height_60"
        android:paddingStart="@dimen/margin16"
        android:paddingEnd="@dimen/margin16"
        android:gravity="center_vertical"
        app:drawableTint="?attr/colorArrowRight"
        style="@style/Regular16Text"
        android:text="@string/filter_country"
        android:textColor="@color/hh_grey"
        android:textSize="16sp"
        android:background="@null"
        app:drawableEndCompat="@drawable/ic_arrow_right"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
